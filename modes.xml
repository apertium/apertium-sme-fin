      </program>
      <program name="apertium-postchunk">
        <file name="apertium-sme-fin.sme-fin.t3x"/>
        <file name="sme-fin.t3x.bin"/>
      </program>
      <program name="hfst-proc $1">
        <file name="sme-fin.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>

